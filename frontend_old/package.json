{
  "name": "hex_front",
  "version": "6.0.0",
  "description": "Ready-to-go Preact starter project powered by webpack.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && jest --coverage",
    "test:watch": "npm run -s test -- --watch",
    "lint": "eslint src test"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "testURL": "http://localhost:8080",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel": "^6.5.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^20.0.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.11.6",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.5.3",
    "core-js": "^2.5.7",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^20.0.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.0",
    "json-loader": "^0.5.4",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.4",
    "offline-plugin": "^5.0.5",
    "postcss-loader": "^2.1.6",
    "preact-jsx-chai": "^2.2.1",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.10.3",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "sass-loader": "^6.0.7",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.18.1",
    "url-loader": "^0.6.1",
    "webpack": "^3.12.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^2.11.3",
    "worker-loader": "^1.1.1"
  },
  "dependencies": {
    "accessible-autocomplete": "^1.6.1",
    "chart.js": "^2.7.2",
    "material-design-lite": "^1.3.0",
    "preact": "^8.3.1",
    "preact-chartjs-2": "^1.0.3",
    "preact-compat": "^3.18.4",
    "preact-mdl": "^2.2.2",
    "preact-portal": "^1.1.3",
    "preact-render-to-string": "^3.8.2",
    "preact-router": "^2.6.1",
    "preact-token-input": "^0.3.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^1.0.0",
    "react-autocomplete": "^1.8.1",
    "react-calendar-heatmap": "^1.7.0",
    "react-chartjs": "^1.2.0",
    "react-inline-suggest": "^1.0.0",
    "serve": "^10.0.2"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  }
}
